      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@hd-web/build-package": "^0.1.0",
    "@michijs/htmltype": "^0.2.18",
    "csstype": "^3.1.3"
  },
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "type": "module"
}
