{
  "name": "@hd-web/jsx",
  "version": "0.3.0",
  "description": "Provides jsx for building sites with hd-web",
  "scripts": {
    "build": "tsc",
    "test": "node --import tsx --test \"src/**/*.test.ts\""
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@michijs/htmltype": "^0.4.2",
    "csstype": "^3.1.3"
  },
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/hrd543/hd-web"
  }
}
