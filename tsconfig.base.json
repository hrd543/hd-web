{
  "compilerOptions": {
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "isolatedModules": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "lib": ["dom", "esnext"],
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "declaration": true,
    "jsx": "preserve",
    "jsxImportSource": "@hd-web/jsx",
    "skipLibCheck": true,
    "noImplicitOverride": true
  }
}
