{
  "name": "@hd-web/jsx",
  "version": "0.1.0",
  "description": "Provides types and functionality to turn jsx into an html string.",
  "scripts": {
    "build": "npx @hd-web/build-package && tsc",
    "test": "node --import tsx --test \"src/**/*.test.ts\""
  },
  "exports": {
